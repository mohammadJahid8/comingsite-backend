{
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },

  "extends": ["eslint:recommended", "plugin:node/recommended", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "error",
    "no-console": "warn",
    "no-undef": "error",
    "no-unused-expressions": "error",
    "no-unreachable": "error",
    "node/no-unsupported-features/es-syntax": "off"
  }
}
